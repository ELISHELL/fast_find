{
  "name": "fast_find",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@10.18.3",
  "description": "a fast file finder powered by typescript",
  "author": "pockpock",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/ELISHELL/fast_find.git"
  },
  "keywords": [],
  "main": "dist/src/index.js",
  "bin": {
    "fast_find": "bin/fast_find.mjs"
  },
  "files": [
    "bin/",
    "dist/",
    "package.json",
    "readme.md",
    "src/"
  ],
  "scripts": {
    "build": "tsc -b --clean && tsc -b --force",
    "dev": "tsx ./src/cli.ts",
    "cmd": "tsc && node dist/app/cli.js ",
    "cmd_h": "tsc && node dist/app/cli.js -h",
    "cmd_v": "tsc && node dist/app/cli.js -v",
    "cmd_s": "tsc && node dist/app/cli.js search log.ts -p TreePlugin -p !ScopePlugin",
    "cmd_s_h": "tsc && node dist/app/cli.js search -h",
    "cmd_i": "tsc && node dist/app/cli.js i TreePlugin",
    "cmd_i_h": "tsc && node dist/app/cli.js i -h",
    "cmd_u": "tsc && node dist/app/cli.js use TreePlugin",
    "cmd_u_h": "tsc && node dist/app/cli.js use -h",
    "test": "vitest"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.9.0",
    "@types/yargs": "^17.0.34",
    "chalk": "^5.6.2",
    "figlet": "^1.9.3",
    "ignore": "^7.0.5",
    "inquirer-file-selector": "^1.0.1",
    "single-line-log": "^1.1.2",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.2.0",
    "@types/node": "^24.9.1",
    "@types/single-line-log": "^1.1.2",
    "eslint-plugin-format": "^1.0.2",
    "eslint-plugin-n": "^17.23.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.3"
  }
}
